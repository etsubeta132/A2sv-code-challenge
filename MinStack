class MinStack(object):
    def __init__(self):
        self.list = []
        self.num = None
        self.min = 0

    def push(self, val):
        """
        :type val: int
        :rtype: None
        """
        self.list.append(val)
        self.num = val

    def pop(self):
        """
        :rtype: None
        """
        self.list.pop()

    def top(self):
        """
        :rtype: int
        """
        return self.list[-1]

    def getMin(self):
        """
        :rtype: int
        """
        min_num = min(self.list)
        return min_num
        


# Your MinStack object will be instantiated and called as such:
# obj = MinStack()
# obj.push(val)
# obj.pop()
# param_3 = obj.top()
# param_4 = obj.getMin()
